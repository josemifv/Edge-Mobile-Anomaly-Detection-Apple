# Edge Mobile Anomaly Detection - Configuration Defaults
# Copy this file to .env and modify values as needed

# Benchmark Configuration
BENCHMARK_OUTPUT_DIR=outputs/benchmarks
MAX_WORKERS=8

# OSP Algorithm Parameters
OSP_N_COMPONENTS=3
OSP_ANOMALY_THRESHOLD=2.0

# Pipeline Configuration
DEFAULT_INPUT_DIR=data/raw
DEFAULT_OUTPUT_DIR=outputs

# System Optimization (Apple Silicon)
# These are automatically detected but can be overridden
# PYTORCH_MPS_ENABLED=1
# ACCELERATE_MIXED_PRECISION=no

# Logging
LOG_LEVEL=INFO
